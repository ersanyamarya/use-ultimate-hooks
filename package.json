{
  "name": "use-ultimate-hooks",
  "version": "0.1.0",
  "license": "GNU GENERAL PUBLIC LICENSE",
  "author": "Sanyam Arya",
  "main": "dist/index.js",
  "module": "dist/use-ultimate-hooks.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 120,
    "proseWrap": "never",
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/use-ultimate-hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/use-ultimate-hooks.esm.js",
      "limit": "10 KB"
    }
  ]
}
